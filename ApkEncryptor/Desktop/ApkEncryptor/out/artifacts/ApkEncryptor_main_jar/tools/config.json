{
    "secureOptions": {
        "encryptionKey": "PROTECTOR2021"
    },
    "hideApkProtector": {
        "alphabet": "qwertyuiopasdfghjklzxcvbnm",
        
        "type": 0, // 0 - Disabled; 1 - Custom; 2 - Random; 3 - random and custom
        "packageName": "com.android.protector", // 0 - default; 1, 3 - custom; 2 - random
        "dexPrefix": "classes-v", // 0 - default; 1 - custom; 2,3 - random
        "dexSuffix": ".bin", // 0 - default; 1,3 - custom; 2 - random
        "proxyApp": "MyApplication", // 0 - default; 1, 3 - custom; 2 - random
        "assetsDirDex": "dexes" // 0 - default; 1, 3 - custom; 2 - random
    }
}