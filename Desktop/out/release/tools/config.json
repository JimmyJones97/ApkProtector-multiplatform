{
    /*
     * SETTINGS: APK SIGNER AND ALIGNER
     */
    "keystore":{
        /**
         * APK ALIGNER
         */
        "zipaligner":true,
        
        /**
         * KEYSTORE FOR APK SIGNING
         */
        "keystorePath":"debug.jks",
        "keystorePassword":"debug",
        "keystoreAlias":"debug",
        "certPassword":"debug",
        
        /**
         * APK SIGNER
         */
        "signatureV1Enabled":true,
        "signatureV2Enabled":true,
        "signatureV3Enabled":false,
        "signatureV4Enabled":false
    },
    /*
     * SETTINGS: DEX ENCRYPTION
     */
    "secureOptions": {
        /**
         * ENTER PROTECTED DEXES PASSWORD. RECOMMENDED 8 OR MORE SYMBOLS
         */
        "encryptionKey": "PROTECTOR2021"
    },
    /*
     * SETTINGS: HIDE APKPROTECTOR
     */
    "hideApkProtector": {
        /**
         * ENTER ALPHABET TO GENERATE RANDOM NAMES
         */
        "alphabet": "qwertyuiopasdfghjklzxcvbnm",
        
        /**
         * SELECT BUILD TYPE [0-3]
         * 0 - DEFAULT NAMES
         * 1 - CUSTOM NAMES
         * 2 - RANDOM NAMES
         * 3 - RANDOM NAMES TO: dexFolder, dexPrefix; CUSTOM NAMES TO: packageName, dexSuffix, proxyApp.
         */
        "type": 0,
        /**
         * ENTER PACKAGE NAME FOR DEXLOADER
         */
        "packageName": "com.android.protectorW",
        /**
         * ENTER DEX FOLDER NAME IN ASSETS
         */
        "dexFolder": "protectW",
        /**
         * ENTER PREFIX NAME FROM PROTECTED DEXES
         */
        "dexPrefix": "protect-vW",
        /**
         * ENTER SUFFIX NAME FROM PROTECTED DEXES
         */
        "dexSuffix": ".encW",
        /**
         * ENTER APPLICATION CLASS NAME
         */
        "proxyApp": "ProxyApplicationW"
    }
}