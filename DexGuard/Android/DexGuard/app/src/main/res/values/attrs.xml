<?xml version="1.0" encoding="utf-8"?>
<resources>
<declare-styleable name="FabSpeedDial">
        <!-- Menu resource -->
        <attr name="fab_menu" format="reference"/>

        <!-- Main FAB background color -->
        <attr name="fab_fabBackgroundColor" format="color"/>
        <!-- Main FAB drawable -->
        <attr name="fab_fabDrawable" format="reference"/>
        <!-- Main FAB drawable tint -->
        <attr name="fab_fabDrawableTint" format="color"/>
        <!-- Main FAB ripple color -->
        <attr name="fab_fabRippleColor" format="color"/>
        <!-- Main FAB rotation -->
        <attr name="fab_fabRotationAngle" format="float"/>

        <!-- Mini FAB background color -->
        <attr name="fab_miniFabBackgroundColor" format="color"/>
        <attr name="fab_miniFabBackgroundColorList" format="reference"/>
        <!-- Main FAB drawable tint -->
        <attr name="fab_miniFabDrawableTint" format="color"/>
        <attr name="fab_miniFabDrawableTintList" format="reference"/>
        <!-- Mini FAB ripple color -->
        <attr name="fab_miniFabRippleColor" format="color"/>
        <attr name="fab_miniFabRippleColorList" format="reference"/>

        <!-- Mini FAB text color -->
        <attr name="fab_miniFabTextColor" format="color"/>
        <attr name="fab_miniFabTextColorList" format="reference"/>
        <!-- Mini FAB text background drawable -->
        <attr name="fab_miniFabTextBackground" format="reference"/>
        <attr name="fab_miniFabTextBackgroundList" format="reference"/>

        <!-- Use touch guard -->
        <attr name="fab_useTouchGuard" format="boolean"/>
        <!-- Touch guard color -->
        <attr name="fab_touchGuardColor" format="color"/>
        <!-- Use touch guard reveal effect -->
        <attr name="fab_useRevealEffect" format="boolean"/>

        <!-- Use rippe on pre Lollipop -->
        <attr name="fab_useRippleOnPreLollipop" format="boolean"/>

        <!-- Extra margin between FABs -->
        <attr name="fab_extraMargin" format="dimension"/>

        <!-- Show menus horizontally on landscape mode -->
        <attr name="fab_showHorizontallyOnLandscape" format="boolean"/>
    </declare-styleable>
 <declare-styleable name="ParticleView">
        <attr name="pv_host_text" format="string"></attr>
        <attr name="pv_host_text_size" format="dimension"></attr>
        <attr name="pv_particle_text" format="string"></attr>
        <attr name="pv_particle_text_size" format="dimension"></attr>
        <attr name="pv_text_color" format="color"></attr>
        <attr name="pv_background_color" format="color"></attr>
        <attr name="pv_text_anim_time" format="integer"></attr>
        <attr name="pv_spread_anim_time" format="integer"></attr>
        <attr name="pv_host_text_anim_time" format="integer"></attr>
    </declare-styleable>
 <declare-styleable name="FilePickerPreference">
        <attr name="selection_mode" format="enum">
            <enum name="single_mode" value="0"/>
            <enum name="multi_mode" value="1"/>
        </attr>
        <attr name="selection_type" format="enum">
            <enum name="file_select" value="0"/>
            <enum name="dir_select" value="1"/>
            <enum name="file_dir_select" value="2"/>
        </attr>

        <attr
            name="root_dir"
            format="string"/>

        <attr
            name="error_dir"
            format="string"/>

        <attr
            name="offset_dir"
            format="string"/>

        <attr
            name="extensions"
            format="string"/>

        <attr
            name="title_text"
            format="string"/>

    </declare-styleable>
    <declare-styleable name="FloatingActionButton">
        <attr name="fab_colorNormal" format="color" />
        <attr name="fab_colorPressed" format="color" />
        <attr name="fab_colorDisabled" format="color" />
        <attr name="fab_colorRipple" format="color" />
        <attr name="fab_showShadow" format="boolean" />
        <attr name="fab_shadowColor" format="color" />
        <attr name="fab_shadowRadius" format="dimension" />
        <attr name="fab_shadowXOffset" format="dimension" />
        <attr name="fab_shadowYOffset" format="dimension" />
        <attr name="fab_size" format="enum">
            <enum name="normal" value="0" />
            <enum name="mini" value="1" />
        </attr>
        <attr name="fab_showAnimation" format="reference" />
        <attr name="fab_hideAnimation" format="reference" />
        <attr name="fab_label" format="string" />
        <attr name="fab_elevationCompat" format="dimension" />
        <attr name="fab_progress_color" format="color" />
        <attr name="fab_progress_backgroundColor" format="color" />
        <attr name="fab_progress_indeterminate" format="boolean" />
        <attr name="fab_progress_max" format="integer" />
        <attr name="fab_progress" format="integer" />
        <attr name="fab_progress_showBackground" format="boolean" />
    </declare-styleable>

    <declare-styleable name="FloatingActionMenu">
        <attr name="menu_showShadow" format="boolean" />
        <attr name="menu_buttonSpacing" format="dimension" />
        <attr name="menu_labels_margin" format="dimension" />
        <attr name="menu_labels_showAnimation" format="reference" />
        <attr name="menu_labels_hideAnimation" format="reference" />
        <attr name="menu_labels_paddingTop" format="dimension" />
        <attr name="menu_labels_paddingLeft" format="dimension" />
        <attr name="menu_labels_paddingRight" format="dimension" />
        <attr name="menu_labels_paddingBottom" format="dimension" />
        <attr name="menu_labels_padding" format="dimension" />
        <attr name="menu_labels_textColor" format="reference|color" />
        <attr name="menu_labels_textSize" format="dimension" />
        <attr name="menu_labels_cornerRadius" format="dimension" />
        <attr name="menu_labels_showShadow" format="boolean" />
        <attr name="menu_labels_colorNormal" format="color" />
        <attr name="menu_labels_colorPressed" format="color" />
        <attr name="menu_labels_colorRipple" format="color" />
        <attr name="menu_labels_position" format="enum">
            <enum name="left" value="0" />
            <enum name="right" value="1" />
        </attr>
        <attr name="menu_icon" format="reference" />
        <attr name="menu_animationDelayPerItem" format="integer" />
        <attr name="menu_buttonToggleAnimation" format="reference" />
        <attr name="menu_labels_singleLine" format="boolean" />
        <attr name="menu_labels_ellipsize" format="enum">
            <enum name="none" value="0" />
            <enum name="start" value="1" />
            <enum name="middle" value="2" />
            <enum name="end" value="3" />
            <enum name="marquee" value="4" />
        </attr>
        <attr name="menu_labels_maxLines" format="integer" />
        <attr name="menu_fab_size" format="enum">
            <enum name="normal" value="0" />
            <enum name="mini" value="1" />
        </attr>
        <attr name="menu_labels_style" format="reference" />
        <attr name="menu_labels_customFont" format="string" />
        <attr name="menu_shadowColor" format="color" />
        <attr name="menu_shadowRadius" format="dimension" />
        <attr name="menu_shadowXOffset" format="dimension" />
        <attr name="menu_shadowYOffset" format="dimension" />
        <attr name="menu_colorNormal" format="color" />
        <attr name="menu_colorPressed" format="color" />
        <attr name="menu_colorRipple" format="color" />
        <attr name="menu_openDirection" format="enum">
            <enum name="up" value="0" />
            <enum name="down" value="1" />
        </attr>
        <attr name="menu_backgroundColor" format="color" />
        <attr name="menu_fab_label" format="string" />
        <attr name="menu_fab_show_animation" format="reference" />
        <attr name="menu_fab_hide_animation" format="reference" />
    </declare-styleable>
 <declare-styleable name="UltraViewPager">
        <attr name="upv_autoscroll" format="integer" />
        <attr name="upv_infiniteloop" format="boolean" />
        <attr name="upv_ratio" format="float" />
        <attr name="upv_scrollmode" format="enum">
            <enum name="horizontal" value="0" />
            <enum name="vertical" value="1" />
        </attr>
        <attr name="upv_disablescroll" format="enum">
            <enum name="none" value="0" />
            <enum name="backward" value="1" />
            <enum name="forward" value="2" />
        </attr>
        <attr name="upv_multiscreen" format="float" />
        <attr name="upv_automeasure" format="boolean" />
        <attr name="upv_itemratio" format="float" />
    </declare-styleable>
    <declare-styleable name="ExpansionHeader">
        <attr name="expansion_layout" format="reference"/>
        <attr name="expansion_toggleOnClick" format="boolean"/>
        <attr name="expansion_headerIndicator" format="reference"/>
        <attr name="expansion_headerIndicatorRotationExpanded" format="integer"/>
        <attr name="expansion_headerIndicatorRotationCollapsed" format="integer"/>
    </declare-styleable>

    <declare-styleable name="ExpansionLayout">
        <attr name="expansion_expanded" format="boolean"/>
    </declare-styleable>

    <attr name="expansion_openOnlyOne" format="boolean"/>

    <declare-styleable name="ExpansionsViewGroupFrameLayout">
        <attr name="expansion_openOnlyOne"/>
    </declare-styleable>
    <declare-styleable name="ExpansionsViewGroupLinearLayout">
        <attr name="expansion_openOnlyOne"/>
    </declare-styleable>
    <declare-styleable name="ExpansionsViewGroupRelativeLayout">
        <attr name="expansion_openOnlyOne"/>
    </declare-styleable>
    <declare-styleable name="ExpansionsViewGroupConstraintLayout">
        <attr name="expansion_openOnlyOne"/>
    </declare-styleable>
</resources>